
{
    "version": 2,
    "builds": [
        {
            "src": "workstation-server/api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "workstation-server/api/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
